{
  "name": "enneagram-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern && browserlist > docs/supported-browsers.txt",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --write **/*.{ts,js,vue,css,less,sass,scss,html,json,md}",
    "prettier-watch": "onchange 'src/**/*.{ts,js,vue,css,less,scss,html,json,md}' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "bootstrap-vue": "^2.0.0-rc.27",
    "core-js": "^2.6.5",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-pwa": "^3.9.0",
    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-service": "^3.9.3",
    "lint-staged": "^9.2.1",
    "prettier": "1.18.2",
    "sass": "^1.18.0",
    "sass-loader": "^7.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,js,vue,css,less,sass,scss,html,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
